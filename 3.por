programa {
  funcao inicio() {
inteiro num1,num2
        escreva("Digite um numero: \n")
    leia(num1)
    escreva("Agota digite outro: \n")
    leia(num2)

    se(num1 > num2)
    escreva("O primeiro numero é maior")
    senao 
    se(num1 < num2)
    escreva("O segundo numero é maior")
    senao 
    escreva("Os numeros são iguais")

  }
}
